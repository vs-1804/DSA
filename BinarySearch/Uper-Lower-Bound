#Upper Bound of a element in sortef array
public static int upperBinary(int[] arr, int target) {
		int left= 0;
		int ans=-1;
		int right = arr.length - 1;
		while (left <= right) {
			int mid=left+(right-left)/2;
			if(arr[mid]==target) {
				ans=mid;
				left=mid+1;
			}
			else if(arr[mid]<target) {
				left=mid+1;
			}
			else{
				right=mid-1;
			}
		}
		return ans;
	}

